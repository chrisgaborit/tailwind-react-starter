{
  "name": "genesis-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "dev:backend": "npm --prefix backend/backend run dev",
    "dev:frontend": "npm --prefix frontend run dev",
    "build": "npm --prefix backend/backend run build && npm --prefix frontend run build",
    "lint": "npm --prefix frontend run lint",
    "typecheck": "npm --prefix frontend run typecheck"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
